{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "keyauto",
  "version": "0.1.0",
  "identifier": "superwindcloud",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:3333",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "plugins": {
    "splashscreen": {
      "enabled": true
    }
  },
  "app": {
    "windows": [
      {
        "title": "keyauto",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "visible": false ,
        "label": "main"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "publisher" : "Superwindcloud",
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png"
    ]
  }
}
